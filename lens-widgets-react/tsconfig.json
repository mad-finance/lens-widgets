{
  "compilerOptions": {
    "module": "esnext",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "jsx": "react-jsx",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "outDir": "./dist",
    "resolveJsonModule": true
  },
  "include": ["./src"],
  "exclude": ["node_modules/", "lens-oa-client/"]
}